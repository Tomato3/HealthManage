<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_109"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="MissingConstraints">

    <ImageView
        android:id="@+id/iv_health_report_avatar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_health_report_logo"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/tv_health_report_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="周健康报告"
        android:textColor="@color/colorTxtBlack"
        android:textSize="@dimen/sp_15"
        app:layout_constraintLeft_toRightOf="@id/iv_health_report_avatar"
        app:layout_constraintTop_toTopOf="@id/iv_health_report_avatar"
        android:layout_marginLeft="@dimen/dp_18"/>

    <TextView
        android:id="@+id/tv_health_report_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="创建时间"
        android:textSize="@dimen/sp_15"
        app:layout_constraintLeft_toRightOf="@id/iv_health_report_avatar"
        app:layout_constraintBottom_toBottomOf="@id/iv_health_report_avatar"
        android:layout_marginLeft="@dimen/dp_18"/>

    <ImageView
        android:id="@+id/iv_to_report_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/more_icon"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>