<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="ViewModel"
            type="com.example.healthmanage.ui.activity.searchMember.SearchMemberViewModel" />
    </data>

    <RelativeLayout style="@style/AllMatchWhiteLayoutStyle">

        <include
            android:id="@+id/include_search"
            style="@style/ActivitySearchMemberIncludeStyle"
            layout="@layout/include_search" />

        <TextView
            android:id="@+id/tv_cancel"
            style="@style/ActivitySearchMemberTxtStyle.Cancel" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recycler_view_search_member"
            style="@style/ActivitySearchMemberRecyclerViewStyle" />
    </RelativeLayout>

</layout>